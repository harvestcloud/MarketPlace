{% extends 'HarvestCloudMarketPlaceProfileBundle::layout.html.twig' %}
{% form_theme form 'HarvestCloudMarketPlaceCoreBundle:Form:fields.html.twig' %}

{% block content %}
    <h1>Edit {{ profile }}</h1>
    <form action="{{ path('Profile_update') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="form row">
            <input type="submit" class="submit button" value="Save" />
        </div><!-- .form.row -->
    </form>
{% endblock %}
